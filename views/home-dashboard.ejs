          </div>
          <div class="modal-body">
            <% bookErrors.forEach(function(message) { %>
            <div
              class="alert alert-danger small"
              style="padding: 5px; font-size: 12px; margin-bottom: 0"
            >
              <%= message %>
            </div>
            <% }) %>
            <form id="myForm1" action="/book" method="post">
              <label for="selectedInput">You have selected: </label>
              <input
                type="text"
                id="selectedInput"
                value="Sham Turf"
                placeholder="SHAM TURF"
                name="turf"
                readonly
              /><br />
              <label for="exampleInputName1" class="form-label"
                >Booking name</label
              >
              <input
                type="text"
                name="name"
                autocomplete="off"
                class="form-control"
                id="exampleInputName1"
              />
              <label for="exampleInputEmail1" class="form-label"
                >Email address</label
              >
              <input
                type="email"
                name="bookingEmail"
                autocomplete="off"
                class="form-control"
                id="exampleInputEmail1"
                aria-describedby="emailHelp"
              />
              <label for="exampleInputPhone1" class="form-label"
                >Phone No</label
              >
              <input
                type="number"
                name="bookingNumber"
                autocomplete="off"
                class="form-control"
                id="exampleInputNumber1"
              />
              <label for="exampleInputDate1" class="form-label">Date</label>
              <input
                type="date"
                name="bookingDatetime"
                autocomplete="off"
                class="form-control"
                id="exampleInputDate1"
              />
              <label for="exampleInputSlots1" class="form-label">Slots</label>
              <select
                name="slots"
                class="form-select"
                aria-label="Default select example"
                id="exampleInputSlot1"
              >
                <option selected>Choose Slots</option>
                <option value="7AM-9AM">7AM-9AM</option>
                <option value="9AM-11AM">9AM-11AM</option>
                <option value="11AM-2PM">11AM-2PM</option>
                <option value="2PM-4PM">2PM-4PM</option>
                <option value="4PM-7PM">4PM-7PM</option>
                <option value="4PM-7PM">7PM-8PM</option>
              </select>
              <input
                type="hidden"
                name="redirectLink"
                value="https://buy.stripe.com/test_14k00ZbXb4Q4gHm3cc"
              />
              <button
                style="margin-top: 5px; text-align: center"
                type="submit"
                class="btn btn-primary"
              >
                BOOK NOW!
              </button>
            </form>
          </div>
          <div class="modal-footer">
            <button
              type="button"
              class="btn btn-secondary"
              data-bs-dismiss="modal"
            >
              Close
            </button>
          </div>
        </div>
      </div>
    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe"
      crossorigin="anonymous"
    ></script>
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
      AOS.init({
        duration: 1500,
      });
    </script>
    <script src="/main-bundled.js"></script>
  </body>
</html>
