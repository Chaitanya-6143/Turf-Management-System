<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>CTS</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/style.css" />
    <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />
  </head>
  <body>
    <div class="home">
      <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
          <a class="navbar-brand">CTS</a>
          <button
            class="navbar-toggler"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <li class="nav-item">
                <a class="nav-link active" aria-current="page" href="#">Home</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">About</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#turfs">Turfs</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">Contact-Us</a>
              </li>
            </ul>

            <button
              type="button"
              class="btn btn-primary"
              data-bs-toggle="modal"
              data-bs-target="#staticBackdrop"
            >
              LOGIN
            </button>
          </div>
        </div>
      </nav>

      <% errors.forEach(function(message) { %>
      <div class="alert alert-danger text-center"><%= message %></div>
      <% }) %>

      <div class="whole-form" data-aos="flip-left">
        <h3>Sign up here</h3>
        <% regErrors.forEach(function(message) { %>
        <div
          class="alert alert-danger small"
          style="padding: 5px; font-size: 12px; margin-bottom: 0"
        >
          <%= message %>
        </div>
        <% }) %>
        <form action="/register" method="POST" id="registration-form">
          <div class="mb-3">
            <label for="exampleInputEmail1" class="form-label"
              >Email address</label
            >
            <input
              type="email"
              name="email"
              autocomplete="off"
              class="form-control"
              id="exampleInputEmail1"
              aria-describedby="emailHelp"
            />
            <div id="emailHelp" class="form-text">
              We'll never share your email with anyone else.
            </div>
          </div>
          <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label"
              >Password</label
            >
            <input
              type="password"
              name="password"
              autocomplete="off"
              class="form-control"
              id="exampleInputPassword1"
            />
          </div>
          <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label"
              >Phone number</label
            >
            <input
              type="number"
              id="exampleInputPhone1"
              name="phoneno"
              autocomplete="off"
              class="form-control"
            />
          </div>
          <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">City</label>
            <input
              type="text"
              id="exampleInputCity1"
              class="form-control"
              name="city"
              autocomplete="off"
            />
          </div>
          <button type="submit" class="btn btn-primary">Submit</button>
        </form>
      </div>
    </div>
    <div class="welcome" id="turfs">
      <h2 data-aos="slide-right">Play at best <b>Turf</b> in your city</h2>
    </div>
    <div class="turf1" data-aos="zoom-in-left">
      <div class="turf1Pic"></div>
      <div class="turf1Content">
        <div class="turf1Content1">
          <h3>Shyam Turf</h3>
        </div>
        <h5 style="margin-left: 10px">Welcome to Shyam Turf</h5>
        <div class="star-rating">
          <h6>3.0</h6>
          <span class="star filled"></span>
          <span class="star filled"></span>
          <span class="star filled"></span>
          <span class="star"></span>
          <span class="star"></span>
        </div>
        <p>3 star rating</p>
        <div class="map">
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d29542.592434151407!2d68.94879389314521!3d22.246753523839164!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x39569c266399e37b%3A0xb5866e461a106e0a!2sDwarka%2C%20Gujarat!5e0!3m2!1sen!2sin!4v1683460747353!5m2!1sen!2sin"
            width="75px"
            height="75px"
            style="border: 0"
            allowfullscreen=""
            loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"
          ></iframe>
          <p>located at Dwarka Gujrat</p>
        </div>
        <div class="priceTurf1">
          <p>Price is 1000 per hour</p>
        </div>
        <div class="owner">
          <p>Owner: Shyam Kumar(+917741826373)</p>
        </div>
        <button
          type="button"
          class="btn btn-secondary"
          data-bs-toggle="modal"
          data-bs-target="#exampleModal"
        >
          Available Slots
        </button>
        <div class="turf1Footer">
          <a href="tel:+917741826374"
            ><button type="button" id="button1" class="btn btn-success">
              Enquiry
            </button></a
          >
          <button
            type="button"
            data-bs-toggle="modal"
            data-bs-target="#staticBackdrop"
            class="btn btn-success"
          >
            Book Now
          </button>
        </div>
      </div>
    </div>

    <div class="turf1" data-aos="zoom-in-right">
      <div class="turf2Pic"></div>
      <div class="turf1Content">
        <div class="turf1Content1">
          <h3>Ram Turf</h3>
        </div>
        <h5 style="margin-left: 10px">OPEN FOR 24HRS</h5>
        <div class="star-rating">
          <h6>4.0</h6>
          <span class="star filled"></span>
          <span class="star filled"></span>
          <span class="star filled"></span>
          <span class="star filled"></span>
          <span class="star"></span>
        </div>
        <p>4 star rating</p>
        <div class="map">
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d56985.1840833745!2d82.1579677958838!3d26.789845860695706!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x399a07937e6d2823%3A0x5fc8f683b17f222b!2sAyodhya%2C%20Uttar%20Pradesh!5e0!3m2!1sen!2sin!4v1683539822355!5m2!1sen!2sin"
            width="75"
            height="75"
            style="border: 0"
            allowfullscreen=""
            loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"
          ></iframe>
          <p>located at Ayodha Uttar Pradesh</p>
        </div>
        <div class="priceTurf1">
          <p>Price is 700 per hour</p>
        </div>
        <div class="owner">
          <p>Owner: Ramji(+9145676543456)</p>
        </div>
        <button
          type="button"
          class="btn btn-secondary"
          data-bs-toggle="modal"
          data-bs-target="#exampleModal"
        >
          Available Slots
        </button>
        <div class="turf1Footer">
          <a href="tel:+917741826374"
            ><button type="button" id="button1" class="btn btn-success">
              Enquiry
            </button></a
          >
          ><button
            type="button"
            data-bs-toggle="modal"
            data-bs-target="#staticBackdrop"
            class="btn btn-success"
          >
            Book Now
          </button>
        </div>
      </div>
    </div>
    <div class="contact-form" data-aos="zoom-in-left">
      <h1>CONTACT US</h1>
      <img data-aos="zoom-in-left" src="/contactus.png" />
      <form>
        <div class="mb-3" data-aos="zoom-out-right">
          <input
            type="text"
            class="form-control"
            placeholder="Enter Your Full Name"
            aria-label="Username"
            aria-describedby="basic-addon1"
          />
        </div>
        <div class="mb-3" data-aos="zoom-out-left">
          <input
            type="email"
            placeholder="Enter Your Email"
            class="form-control"
            id="contactus"
            aria-describedby="emailHelp"
          />
        </div>
        <div class="mb-3" data-aos="zoom-out-down">
          <textarea
            placeholder="Enter Your Query Here---->"
            class="form-control"
            id="exampleFormControlTextarea1"
            rows="3"
          ></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
    <div class="aboutus" data-aos="flip-left">
      <h2 data-aos="flip-right">About Us</h2>
      <p data-aos="zoom-in-left">
        e-TURF Management System is very useful to stay organised in the booking
        of turf and slots there can be difficulty in offline booking i.e
        sometimes people arrive at turf before there time slot booked. We can
        get each and every information about any turf we want without visiting
        that turf physically. In the Turf Management system grounds booking is a
        form of electronic commerce which allows consumers to directly buy goods
        or services from a seller over the Internet using a web browser.
        Consumers find a turf of interest by visiting the website of the
        retailer directly or by searching among alternative vendors using a
        shopping search engine, which displays the same turf's availability and
        pricing at different e-retailers.Online stores typically enable shoppers
        to use "search" features to find specific models, brands or items.
        Online customers must have access to the Internet and a valid method of
        payment in order to complete a transaction, such as a credit card, an
        Interac-enabled debit card, or a service such as PayPal. For physical
        hall (eg, paperback books or clothes), the e-tailer ships the hall to
        the customer, for digital hall, such as digital audio files of songs or
        software, the e-tailer typically sends the file to the customer over the
        Internet. The largest of these online retailing corporations are
        Alibaba, Amazon.com, and eBay.
      </p>
    </div>
    <!-- Modal -->
    <div
      class="modal fade"
      id="exampleModal"
      tabindex="-1"
      aria-labelledby="exampleModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h1 class="modal-title fs-5" id="exampleModalLabel">
              Available-Slots
            </h1>
            <button
              type="button"
              class="btn-close"
              data-bs-dismiss="modal"
              aria-label="Close"
            ></button>
          </div>
          <div class="modal-body">
            6AM-7AM (AVAILABLE) | 7AM-9AM (BOOKED) | 9AM-11AM (BOOKED) |
            11AM-2PM (AVAILABLE) | 2PM-4PM (AVAILABLE) | 4PM-7PM (BOOKED) |
            7PM-8PM (AVAILABLE) | 8PM-10PM (BOOKED) | 10PM-12PM (AVAILABLE) |
          </div>
          <div class="modal-footer">
            <button
              type="button"
              class="btn btn-secondary"
              data-bs-dismiss="modal"
            >
              OKAY
            </button>
          </div>
        </div>
      </div>
    </div>

    <!-- Modal -->
    <div
      class="modal fade"
      id="staticBackdrop"
      data-bs-backdrop="static"
      data-bs-keyboard="false"
      tabindex="-1"
      aria-labelledby="staticBackdropLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h1 class="modal-title fs-5" id="staticBackdropLabel">
              LOGIN NOW!!!!
            </h1>
            <button
              type="button"
              class="btn-close"
              data-bs-dismiss="modal"
              aria-label="Close"
            ></button>
          </div>
          <div class="modal-body">
            <form action="/login" method="POST" id="login-form">
              <div class="mb-3">
                <label for="exampleInputEmail2" class="form-label"
                  >Email address</label
                >
                <input
                  type="email"
                  name="email"
                  class="form-control"
                  id="exampleInputEmail2"
                  aria-describedby="emailHelp"
                  autocomplete="off"
                  placeholder="Enter your Email"
                />
                <div id="emailHelp" class="form-text">
                  We'll never share your email with anyone else.
                </div>
              </div>
              <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label"
                  >Password</label
                >
                <input
                  type="password"
                  name="password"
                  class="form-control"
                  id="exampleInputPassword2"
                  autocomplete="off"
                  placeholder="Enter password you created"
                />
              </div>
              <button type="submit" class="btn btn-primary">Submit</button>
            </form>
          </div>
        </div>
      </div>
    </div>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe"
      crossorigin="anonymous"
    ></script>
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
      AOS.init({
        duration: 1500,
      });
    </script>
    <script src="/main-bundled.js"></script>
  </body>
</html>
